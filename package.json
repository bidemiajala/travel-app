{
  "name": "travel-app",
  "version": "1.0.0",
  "description": "A simple app to plan your trips. It gives real-time weather information, picture of your destination, and temperature using third-party APIs.",
  "main": "webpack.dev.js",
  "scripts": {
    "test": "jest --coverage",
    "test:badges": "npm run test  && jest-coverage-badges",
    "start": "node src/server/index.js",
    "build-prod": "webpack --config webpack.prod.js",
    "build-dev": "webpack-dev-server  --config webpack.dev.js",
    "build-dev-open": "build-dev -- open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bidemiajala/travel-app.git"
  },
  "keywords": [
    "udacity",
    "travel-app",
    "fend",
    "nano-degree"
  ],
  "author": "Bidemi Ajala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bidemiajala/travel-app/issues"
  },
  "homepage": "https://github.com/bidemiajala/travel-app#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "yarn": "^1.22.10"
  },
  "jest": {
    "coverageReporters": [
      "json-summary", 
      "text",
      "lcov"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.4",
    "css-loader": "^3.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "supertest": "^6.1.3",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "workbox-webpack-plugin": "^4.3.1",
    "write-file-webpack-plugin": "^4.5.1"
  }
